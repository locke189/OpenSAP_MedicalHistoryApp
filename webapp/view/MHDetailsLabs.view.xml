<mvc:View xmlns:core="sap.ui.core" 
		  xmlns:mvc="sap.ui.core.mvc" 
		  xmlns="sap.m" 
		  controllerName="demo_app.com.controller.MedicalHistoryDetail"
		  xmlns:html="http://www.w3.org/1999/xhtml">

	<Table noDataText="Drop column list items here and columns in the area above" id="__table0">

	    <columns>
	        <Column id="__column0">
	            <header>
	                <Label text="ID" id="__label1"/>
	            </header>
	        </Column>
	        <Column id="__column1">
	            <header>
	                <Label text="Lab Type" id="__label2"/>
	            </header>
	        </Column>
	        <Column id="__column2">
	            <header>
	                <Label text="Date" id="__label3"/>
	            </header>
	        </Column>
	        <Column xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" xmlns:footerbar="sap.ushell.ui.footerbar" id="__column2_copy">
	            <header>
	                <Label text="Description" id="__label3_copy"/>
	            </header>
        	</Column>
        </columns>
    
    	<items>
	        <ColumnListItem counter="0" id="__item0">
	            <cells>
	                <Link text="{LabCode}" id="__link0"/>
	                <Text text="{LabAttachment};" maxLines="0" id="__text6"/>
	                <Text text="{Date};" maxLines="0" id="__text7"/>
	                <Text text="{LabAttachment}" maxLines="0" id="__text8"/></cells>
	        </ColumnListItem>
	    </items>
    
    </Table>
    
    
</mvc:View>